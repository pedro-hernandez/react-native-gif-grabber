{
  "expo": {
    "name": "Reaction GIF Generator",
    "description": "Yes? No? Maybe? This app fetches a perfectly random animated GIF to match your mood. GIFs courtesy of Giphy.",
    "slug": "react-native-gif-grabber",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
